class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums)
    {
        vector<int>ans(nums.size(),1);
        long pre=1;
        for(int i=0;i<nums.size();i++)
        {
            ans[i]=(int)pre;
            pre*=nums[i];
        }
        long pos=1;
        for(int i=nums.size()-1;i>=0;i--)
        {
            ans[i]*=(int)pos;
            pos*=nums[i];
        }
        return ans;
    }
};
